{
    "name": "kiwi-suite/template",
    "description": "kiwi-suite/template",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lucas Seinfeld"
        },
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "kiwi-suite/contract": "dev-develop",
        "zendframework/zend-expressive-platesrenderer": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0",
        "kiwi-suite/servicemanager": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateMisc\\Template\\": "tests/misc/",
            "IxocreateTest\\Template\\": "tests/src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix"
    }
}
